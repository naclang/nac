// Example 3: Prime Number Check
// ---------------------------------------------------

fn is_prime(num) {
    if(num < 2) {
        rn 0;
    };
    
    for(i = 2; i * i <= num; i++) {
        if(num % i == 0) {
            rn 0;
        };
    };
    
    rn 1;
};

out("Prime numbers between 2 and 30:");
for(number = 2; number <= 30; number++) {
    if(is_prime(number)) {
        out(number);
    };
};
