// NaC Dili - Kullanici Girisi Ornegi

// Faktoriyel fonksiyonu
fn $f($n){
    ?($n < #2){
        rn #1;
    }:{};
    rn $n * $f($n - #1);
};

// Kullanicidan giris al
$x = in;

// Sonucu yazdir
out($f($x));

// Basit toplama hesaplayici
// Iki sayi al ve topla
$a = in;
$b = in;
$c = $a + $b;
out($c);
