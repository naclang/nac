// NaC Dili - Fibonacci Dizisi

// Ozyinelemeli Fibonacci
fn $f($n){
    ?($n < #2){
        rn $n;
    }:{};
    rn $f($n - #1) + $f($n - #2);
};

// Ilk 10 Fibonacci sayisini yazdir
$i = #0;
@($i; $i < #10; $i++){
    out($f($i));
};

// Dongu ile Fibonacci (daha verimli)
$a = #0;
$b = #1;
$c = #0;
$j = #0;

@($j; $j < #15; $j++){
    out($a);
    $c = $a + $b;
    $a = $b;
    $b = $c;
};
